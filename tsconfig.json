{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": ["es2020"],
    "allowJs": true,
    "rootDir": "src",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
